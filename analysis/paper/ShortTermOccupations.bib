@article{kuijt_bipolar_1995,
	title = {{BIPOLAR} {REDUCTION}: {AN} {EXPERIMENTAL} {STUDY} {OF} {DEBITAGE} {VARIABILITY}},
	volume = {20},
	issn = {01977261},
	abstract = {The archaeological classification of bipolar core reduction has traditionally been achieved through the identification of bipolar cores and individual debitage types assumed to be specific to bipolar reduction. There are, however, two problems with these practices: 1) arguments for the presence of bipolar reduction at individual sites are usually based on core forms; and, 2) recent studies have demonstrated that different reduction techniques can produce similar debitage items. In attempting to overcome these concerns, we have focused on assemblage-level debitage patterning, and undertaken experimental studies to document debitage patterning for bipolar reduction. As a means of graphically comparing experimental results to that of the archaeological assemblage of EeRj 5 in British Columbia, Canada, we have employed Sullivan and Rozen's (1985) method of debitage analysis. Although preliminary, the results of this study indicate that debitage patterning for the experimental and archaeological samples are very similar and that, under certain specific conditions, Sullivan and Rozen's (1985) method of debitage analysis may be useful in augmenting other methods of lithic analysis. Tradicionalmente, la clasificaci\&\#xf3;n de la reducci\&\#xf3;n bipolar ha estado alcanzado por la identificaci\&\#xf3;n de nucleos bipolares y tipos de desperdicios que parecen a ser espec\&\#xed;fico a la reducci\&\#xf3;n bipolar. Sin embargo, hay dos problemas con este m\&\#xe9;todo: 1) argumentos para la presencia de la reducci\&\#xf3;n bipolar en sitios usualmente son fundados en las formas de nucleos; y, 2) estudios recientes han demostrados que las t\&\#xe9;cnicas differentes de reducci\&\#xf3;n pueden producir los mismos tipos de desperdicios. Para superar estas limitaciones, nosotros hemos enfocado a modelos en el conjunto de desperdicios, y hemos hecho experimentos para documentar modelos en los desperdicios de reducci\&\#xf3;n bipolar. Este an\&\#xe1;lisis, aunque preliminar, indica que para los ejemplos experimentales y arqueol\&\#xf3;gicos los modelos de desperdicios son muy similar, y en algunas condici\&\#xf3;nes, se puede usar el m\&\#xe9;todo del an\&\#xe1;lisis de desperdicios de Sullivan y Rozen (1985) para aumentar otros m\&\#xe9;todos del an\&\#xe1;lisis de l\&\#xed;ticos.},
	journal = {Lithic Technology},
	author = {Kuijt, Ian and Prentiss, William C. and Pokotylo, David L.},
	year = {1995},
	pages = {116--127},
	file = {C\:\\Users\\João\\Dropbox (Personal)\\PDF\\Kuijt et al 1995:C\:\\Users\\jmcasca\\Zotero\\storage\\B3QR2P5A\\Kuijt et al 1995.pdf:application/pdf}
